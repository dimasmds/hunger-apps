{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base",
        "plugin:jest/recommended"
    ],
    "plugins": [
        "jest"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-underscore-dangle": 0,
        "jest/no-export": 0
    }
}
